+      const el = document.getElementById(id);
+      if (el) el.remove();
+    });
+  }
+
+  // ---------- Arranque
+  function start(){
+    ensureDefaultRoot();
+    removeLegacyButtons();
+    ensureHeadButton();
+    installHotkeys();
+  }
+  if (document.readyState === "loading") {
+    document.addEventListener("DOMContentLoaded", start);
+  } else {
+    start();
+  }
+})();

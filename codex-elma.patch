+  max-height: var(--verbs-panel-max-h) !important;
+  overflow: auto !important;
+
+  z-index: var(--verbs-panel-z) !important;
+
+  /* visual opcional */
+  box-shadow: 0 12px 28px rgba(0,0,0,.18), 0 2px 8px rgba(0,0,0,.12);

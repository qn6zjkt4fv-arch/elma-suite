+  }
+
+  // ---------- CSS Reset impresi√≥n (ventana)
+  const RESET_CSS = `
+    @page { size:auto; }
+    @media print {
+      html, body { height:auto !important; overflow:visible !important; background:#fff !important; }
+      * { background-image:none !important; }

+
+  /* Si quieres posici√≥n fija en la esquina superior izquierda de la hoja: */
+  /* 
+  .print-brand {
+    position: fixed;
+    top: 10mm;
+    left: 12mm;
+    width: 38mm;
+    height: auto;
+  } 
+  */
+}
+
+/* ===== Dock fijo para la ventana de Verbos (verbs-helper) ===== */
+:root{
+  --verbs-panel-bottom: 6vh;        /* distancia desde borde inferior */
+  --verbs-panel-max-h: 11vh;        /* altura m√°x (‚âà tercio inferior) */
+  --verbs-panel-width: min(980px, 94vw);
+  --verbs-panel-z: 10050;
